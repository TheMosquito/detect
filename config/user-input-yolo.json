{
  "services": [
    {
      "org": "$HZN_ORG_ID",
      "url": "$OWNER_GROUP.mqtt2kafka",
      "versionRange": "[0.0.0,INFINITY)",
      "variables": {
          "EVTSTREAMS_BROKER_URL": "$EVTSTREAMS_BROKER_URL",
          "EVTSTREAMS_API_KEY": "$EVTSTREAMS_API_KEY",
          "EVTSTREAMS_TOPIC": "$EVTSTREAMS_TOPIC",
          "MQTT_TOPIC": "/detect",
	  "MQTT_HOST" : "$OWNER_GROUP.mqtt"
      }
    },
    {
      "org": "$HZN_ORG_ID",
      "url": "$OWNER_GROUP.yolo",
      "versionRange": "[0.0.0,INFINITY)",
      "variables": {
          "YOLO_DEVICENAME": "$DEVICENAME",
	  "MQTT_HOST" : "$OWNER_GROUP.mqtt"
      }
    },
    {
      "org": "$HZN_ORG_ID",
      "url": "$OWNER_GROUP.cam",
      "versionRange": "[0.0.0,INFINITY)",
      "variables": {
	  "MQTT_HOST" : "$OWNER_GROUP.mqtt"
      }
    }
  ]
}
