{
    "label": "biz-policy-yolo-arm",
    "description": "A yolo Business Policy",
    "service": {
	"name": "$EDGE_OWNER.$EDGE_DEPLOY.yolo",
	"org": "$HZN_ORG_ID",
	"arch": "$ARCH",
	"serviceVersions": [
	    {
		"version": "1.0.4",
		"priority":{}
	    }
	]
    },
    "properties": [
    ],
    "constraints": [
	"deploy == $EDGE_DEPLOY",
	"yolo == yes"
    ],
    "userInput": [
	{
	    "serviceOrgid": "$HZN_ORG_ID",
	    "serviceUrl": "$EDGE_OWNER.$EDGE_DEPLOY.yolo",
	    "serviceVersionRange": "[0.0.0,INFINITY)",
	    "inputs": [
		{ "name" : "YOLO_DEVICENAME", "value" : "RPI_TEST"  }
	    ]
	}
    ]
}
